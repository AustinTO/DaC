<!-- Example -->
<group name="local,sysmon,windows">
  <!-- Suppress Sysmon EID 11 'DLL dropped in Temp' when it's CleanMgr -->
  <rule id="100205" level="0">
    <if_matched_sid>92213</if_matched_sid>
    <field name="win.eventdata.image">(?i)cleanmgr\.exe$</field>
    <field name="win.eventdata.targetFilename">(?i)\\AppData\\Local\\Temp\\.*\.dll$</field>
    <description>Exclude benign CleanMgr temp DLL creation</description>
  </rule>
</group>

