<group name="local,sysmon,windows">
  <rule id="100920" level="0">
    <if_sid>92213</if_sid>

    <!-- must be cleanmgr.exe -->
    <field name="win.eventdata.image">(?i)\\cleanmgr\.exe$</field>

    <!-- must be a DLL in a GUID subfolder under Temp -->
    <field name="win.eventdata.targetFilename">(?i)\\AppData\\Local\\Temp\\[A-F0-9\-]{36}\\[^\\]+\.dll$</field>

    <!-- ends with any filename listed in the cleanmgr_dlls list -->
    <list field="win.eventdata.targetFilename" lookup="ends_with">cleanmgr_dlls</list>

    <description>Exclude benign CleanMgr DISM provider DLLs from rule 92213</description>
  </rule>
</group>
